{
    "name": "Styled Weather Sprites TypeDoc (Alieza Design Style)",
    "entryPoints": ["./src"],
    "exclude": [
        "**/__tests__/**/*",
        "**/__test_utils__/**/*",
        "**/__fixtures__/**/*",
        "**/*+(.spec|.e2e).ts",
        "**/+(dev-packages|examples|scripts)/**/*",
        "**/*+(.spec|.e2e).ts"
    ],
    "excludeExternals": true,
    "hideGenerator": true,
    "readme": "README.md",
    "highlightTheme": "dark-plus",
    "out": "docs"
}
