{
    "lerna": "2.2.0",
    "tagVersionPrefix": "lib@",
    "includeMergedTags": true,
    "npmClient": "npm",
    "useWorkspaces": true,
    "version": "1.11.0",
    "packages": ["api/*", "core/*"],
    "command": {
        "run": {
            "stream": true
        },
        "publish": {
            "forcePublish": true,
            "skipGit": true,
            "registry": "https://registry.npmjs.org/"
        }
    }
}
