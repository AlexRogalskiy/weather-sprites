{
    "name": "styled-weather-sprites",
    "version": "0.0.0-dev",
    "description": "Styled weather sprites",
    "license": "GPL-3.0",
    "scripts": {
        "format": "prettier --single-quote --write **/*.ts",
        "format:md": "remark-preset-davidtheclark --format",
        "format:check": "prettier --check **/*.ts",
        "lint": "eslint --cache --fix --quiet --cache-location .cache/ --format codeframe --ext js,ts .",
        "lint:md": "remark-preset-davidtheclark",
        "lint:clean": "del-cli .eslintcache",
        "lint:json": "jsonlint ./package.json --quiet",
        "pretty": "pretty-quick --staged --pattern '!test/tests/lint/**'",
        "license": "license-checker --json > licenses.json",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "markdown": "markdown-link-check ./README.md ./SECURITY.md",
        "remark": "remark -q -f .",
        "clean": "lerna clean --yes",
        "docs": "typedoc",
        "bootstrap": "npm install && lerna bootstrap && lerna run --parallel --stream bootstrap",
        "coverage": "cat ./coverage/lcov.info | coveralls",
        "coverage:open": "opener ./coverage/lcov-report/index.html",
        "postversion": "npm publish --access public",
        "bump:patch": "npm version patch --no-git-tag-version",
        "bump:minor": "npm version minor --no-git-tag-version",
        "bump:major": "npm version major --no-git-tag-version",
        "typecheck": "lerna run --parallel --stream typecheck",
        "publish:latest": "lerna publish --exact && yarn publish:check",
        "next:publish": "lerna publish --exact --canary=next --npm-tag=next --yes",
        "postinstall": "husky install",
        "build": "ncc build core/src/index.ts -m -o dist",
        "package": "ncc build core/src/index.ts --source-map --license licenses.txt",
        "prebuild": "del-cli dist coverage .cache",
        "prepublishOnly": "npm run check:all && npm run build",
        "semantic-release": "semantic-release",
        "test": "env-cmd --silent -e test jest",
        "test:coverage": "env-cmd --silent -e test jest --collectCoverage --detectOpenHandles",
        "test:watch": "env-cmd --silent -e test jest --watch",
        "test:license": "license-checker --production --summary --unknown --onlyAllow=\"Apache-2.0;BSD;ISC;MIT;GPL-3.0;CC-BY-3.0;CC0-1.0;\"",
        "lint:all": "npm run lint:clean && npm run lint && npm run lint:md && npm run lint:json",
        "format:all": "npm run format && npm run format:md",
        "check:all": "npm run format:check",
        "test:all": "npm run test && npm run test:license",
        "all": "npm run format:all && npm run lint:all && npm run pretty && npm run build"
    },
    "dependencies": {
        "env-cmd": "^10.1.0",
        "lodash": "^4.17.20"
    },
    "devDependencies": {
        "@semantic-release/github": "^7.2.0",
        "@semantic-release/npm": "^7.0.10",
        "@semantic-release/release-notes-generator": "^9.0.1",
        "@types/jest": "^26.0.20",
        "@types/lodash.mergewith": "^4.6.6",
        "@types/prettier": "^2.1.5",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "@vercel/ncc": "~>0.27.0",
        "boxen": "^5.0.0",
        "conventional-changelog-cli": "^2.0.0",
        "coveralls": "^3.0.0",
        "dateformat": "^4.5.1",
        "del-cli": "^3.0.1",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-eslint-comments": "^3.1.2",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-spellcheck": "0.0.17",
        "eslint-plugin-unicorn": "^17.2.0",
        "gradient-string": "^1.2.0",
        "husky": "^5.0.8",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jsonlint": "^1.6.3",
        "lerna": "^2.2.0",
        "license-checker": "^25.0.1",
        "markdown-link-check": "^3.8.6",
        "opener": "^1.5.1",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0",
        "randomcolor": "^0.5.4",
        "remark-cli": "^9.0.0",
        "remark-lint-code-block-style": "^2.0.1",
        "remark-lint-ordered-list-marker-value": "^2.0.1",
        "remark-preset-davidtheclark": "^0.12.0",
        "remark-validate-links": "^10.0.2",
        "semantic-release": ">=17.2.3",
        "ts-jest": "^26.4.4",
        "ts-node": ">=9.0.0",
        "typedoc": "^0.20.28",
        "typescript": "^4.1.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexRogalskiy/weather-sprites.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 12.x"
    },
    "keywords": [
        "styled weather sprites",
        "typescript"
    ],
    "maintainers": [
        {
            "name": "Alexander Rogalskiy",
            "email": "hi@nullables.io"
        }
    ],
    "contributors": [
        {
            "name": "Alexander Rogalskiy",
            "email": "hi@nullables.io"
        }
    ],
    "release": {
        "tagFormat": "v${version}",
        "plugins": [
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/npm",
                {
                    "npmPublish": false,
                    "tarballDir": "dist",
                    "allowSameVersion": true
                }
            ],
            [
                "@semantic-release/github",
                {
                    "assets": "dist/*.tgz"
                }
            ]
        ]
    },
    "bugs": {
        "url": "https://github.com/AlexRogalskiy/weather-sprites/issues"
    },
    "homepage": "https://github.com/AlexRogalskiy/weather-sprites",
    "author": {
        "name": "Alexander Rogalskiy",
        "url": "https://github.com/AlexRogalskiy"
    },
    "funding": "https://github.com/sponsors/AlexRogalskiy",
    "files": [
        "api/**",
        "core/**"
    ]
}
